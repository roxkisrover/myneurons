{
  "name": "myneurons",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "antd": "^3.12.1",
    "highcharts": "^7.0.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-jsx-highcharts": "^3.4.0",
    "react-markdown": "^4.0.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react": "^16.7.0",
    "redux-logger": "^3.0.6",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.9",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "react-app-rewired": "^2.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "lint": "eslint \"src/**/*.js\""
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
